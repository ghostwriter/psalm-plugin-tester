{
    "name": "ghostwriter/example-psalm-plugin",
    "description": "An Example plugin for Psalm",
    "type": "psalm-plugin",
    "require": {
        "php": ">=8.1,<8.4",
        "ext-simplexml": "*",
        "composer/semver": "^1.4 || ^2.0 || ^3.0",
        "vimeo/psalm": ">=4.0,<6.0"
    },
    "autoload": {
        "psr-4": {
            "Ghostwriter\\ExamplePsalmPlugin\\": "/"
        }
    },
    "extra": {
        "psalm": {
            "pluginClass": "Ghostwriter\\ExamplePsalmPlugin\\ExamplePlugin"
        }
    },
    "repositories": []
}
